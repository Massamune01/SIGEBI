@model IEnumerable<SIGEBI.Domain.Entities.Configuration.Prestamos.Prestamos>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                IdLibros
            </th>
            <th>
                IdCliente
            </th>
            <th>
                IdLgOpPrest
            </th>
            <th>
                DatePrest
            </th>
            <th>
                DateDevol
            </th>
            <th>
                DateWasDevol
            </th>
            <th>
                Status
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.IdLibros
            </td>
            <td>
				@item.IdCliente
            </td>
            <td>
				@item.IdLgOpPrest
            </td>
            <td>
                @item.DatePrest
            </td>
            <td>
                @item.DateDevol
            </td>
            <td>
                @item.DateWasDevol
            </td>
            <td>
                @item.Status
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>
